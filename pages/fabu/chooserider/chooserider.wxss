.mask{
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: rgba(218, 216, 216, 0.85);
  z-index: 99;
  display: flex;
  justify-content: center;
  align-items: center;
}
.mask .confirmBox{
  height: 500rpx;
  width: 600rpx;
  background-color: #fff;
  border-radius: 10rpx;
  box-sizing: border-box;
  padding: 20rpx;
  color: #000;
  position: relative;
}
.mask .confirmBox .title{
  width: 100%;
  display: flex;
  align-items: center;
  margin-bottom: 17rpx;
}
.mask .confirmBox .title image{
  width: 60rpx;
  height: 60rpx;
  margin-left: 87rpx;
  position: absolute;
  top: 20rpx;
  right: 20rpx;
}
.mask .confirmBox .title view{
  margin-left: 174rpx;
  font-weight: 550;
  font-size: 40rpx;
}
.mask .confirmBox .content view{
  margin-bottom: 27rpx;
  margin-left: 43rpx;
  font-size: 35rpx;
}
.mask .confirmBox .content button{
  margin-top: 10rpx;
  width: 43%;
  background-color: #ec4829;
  color: #fff;
  height: 83rpx;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}