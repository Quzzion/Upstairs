    flex-direction: row;
    width:100%;
  }
  .list .one .user .info{
    width: 83vw;
    margin-left: 20rpx;
  }
  .list .one .user .a{
    display: flex;
  }
  .list .one .user .a text{
    margin-right: 20rpx;
  }
  .list .one .user .b{
    display: flex;
    font-size: 26rpx;
    line-height: 50rpx;
    justify-content: space-between;
  }
  .list .one .user .b .money{
  color: #000;
  }
  .list .one .user .b .orderButton{
   width: 160rpx;
