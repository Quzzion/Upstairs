  line-height: 90rpx;
  color: #fff;
  border-radius: 10rpx;
  font-size: 32rpx;
  position: absolute;
  top: 20rpx;
  right: 20rpx;
  }
  .list .one .col{
    color: #000;
    margin: 5rpx 0;
    /* display: flex;
    align-items: center; */
    font-size: 32rpx;
  }
  .text-fitter{
    font-size: 50rpx;
    text-align: center;

  }
  .list .one .col image{
    width: 30rpx;
    height: 30rpx;
    margin-right: 10rpx;
  }
